@model IEnumerable<CampanhaBD.Model.Importacao>

@{
    ViewBag.Title = "Index";
}

<h3>Importações</h3>

<p>
    @Html.ActionLink("Nova Importação", "Importar")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Data)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Terminado)
        </th>
        <th>
            N° Importados
        </th>
        <th>
            N° Atualizados
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nome)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Data)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Terminado)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumImportados)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NumAtualizados)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
